    <div class="container">
      <p><a class="btn" href="/dicom?refresh=true">Refresh server list</a></p>
      <table>
        <tr>
        <% categories.each do |category| %><th>
          <%= category %>
        </th><% end %>
        </tr>
        <% examinations.each_with_index do |examination, index| %><tr>
          <% examination.each do |field| %><td><a href="/dicom/<%= cryptographic_hashes[index] %>">
          <%= field %>
          </a></td><% end %>
        </tr><% end %>
      </table>
    </div> <!-- /container -->
